    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "axios": "^1.7.9",
    "pdf-lib": "^1.17.1",
    "proj4": "^2.15.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/proj4": "^2.5.5",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
