{
  "name": "cadastre-parcelles",
  "version": "1.0.0",
  "description": "Automatic extraction and visualization of cadastral parcels",
  "type": "module",
  "scripts": {
    "level1": "tsx src/level1.ts",
    "level2": "tsx src/level2.ts",
    "level3": "tsx src/level3.ts",
    "level4": "tsx src/level4.ts",
    "test": "vitest run",
